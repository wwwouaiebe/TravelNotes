{
    "type": "module",
    "name": "travelnotes",
    "version": "4.4.0-alpha.2",
    "buildNumber": 2,
    "description": "A mapping and routing application.",
    "main": "src/main/Main.js",
    "dependencies": {
        "leaflet": "latest"
    },
    "devDependencies": {
        "osrm-text-instructions": "latest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wwwouaiebe/TravelNotes.git"
    },
    "scripts": {
        "release": "node ../AppBuilder/src/AppBuilder.js --type=release",
        "debug": "node ../AppBuilder/src/AppBuilder.js --type=debug",
        "guides": "node ../HtmlFromMarkdown/main.js --src=docs/userGuides",
        "doc": "node ../ESSimpleDoc/bin/index.js --src=./src --dest=./docs/techDoc --validate=true"
    },
    "keywords": [
        "Leaflet",
        "Routing",
        "Markers"
    ],
    "author": "wwwouaiebe",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/wwwouaiebe/TravelNotes/issues"
    },
    "sources": "https://github.com/wwwouaiebe/TravelNotes",
    "homepage": "https://github.com/wwwouaiebe/TravelNotes#readme"
}